<div class="chat-contents">
  <div class="chat-messages">
    <div class="contributor">
      <%= message.user.name %>
    </div>
    <div class="date">
      <!-- 投稿した時刻を出力する -->
      <%= message.created_at %>
    </div>
  </div>
  <div class="text">
    <!-- 投稿したメッセージ内容を記述する -->
    <%= message.content %>
  </div>
</div>